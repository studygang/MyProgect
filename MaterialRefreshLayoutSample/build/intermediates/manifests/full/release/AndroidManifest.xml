        </activity>
        <activity android:name="com.cjj.android_materialrefreshlayout.AutoRefreshActivity" />
        <activity android:name="com.cjj.android_materialrefreshlayout.LoadMoreActivity" />
        <activity android:name="com.cjj.android_materialrefreshlayout.SimpleActivity" />
        <activity android:name="com.cjj.android_materialrefreshlayout.WaveActivity" />
        <activity android:name="com.cjj.android_materialrefreshlayout.SunActivity" />
        <activity android:name="com.cjj.android_materialrefreshlayout.OverLayActivity" />
    </application>

</manifest>